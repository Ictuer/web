<?php

namespace Database\Factories;
<%if(model.prop.some(prop => prop.name == 'password')) echo('use Hash;')_%>

use App\Models\<%=model.name%>;
use Illuminate\Database\Eloquent\Factories\Factory;

class <%=model.name%>Factory extends Factory
{
    /**
     * The name of the factory's corresponding model.
     *
     * @var string
     */
    protected $model = <%=model.name%>::class;

    /**
     * Define the model's default state.
     *
     * @return array
     */
    public function definition()
    {
        return [
            <%_model.prop.where('type', 'not in', ['belongsToMany', 'hasMany', 'id', 'hasOne']).forEach(prop => {_%>
            <%-prop.faker%>
            <%_})_%>
        ];
    }
}
